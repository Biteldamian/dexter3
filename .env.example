# Dexter Configuration
VITE_APP_NAME=Dexter
VITE_APP_VERSION=1.0.0

# API Keys
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key
VITE_PERPLEXITY_API_KEY=your_perplexity_api_key

# File Storage
VITE_STORAGE_PATH=./storage
VITE_MAX_FILE_SIZE=10485760 # 10MB
VITE_ALLOWED_FILE_TYPES=.txt,.md,.pdf,.doc,.docx,.json

# Database
VITE_DB_PATH=./data/dexter.db

# Agent Configuration
VITE_OLLAMA_URL=http://localhost:11434
VITE_DEFAULT_MODEL=llama2
VITE_MAX_CONCURRENT_TASKS=3

# Search Configuration
VITE_SEARXNG_URL=https://searx.be
VITE_SEARCH_ENGINES=google,bing,duckduckgo

# UI Configuration
VITE_THEME=cyberpunk
VITE_ENABLE_ANIMATIONS=true
VITE_SHOW_AGENT_INTERACTIONS=true